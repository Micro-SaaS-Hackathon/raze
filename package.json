{
  "name": "scanmaster",
  "version": "1.0.0",
  "description": "A minimal QR code generator MVP",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "qrcode": "^1.5.3",
    "canvas": "^2.11.2",
    "react-scripts": "^0.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "qr",
    "code",
    "generator",
    "react",
    "node"
  ],
  "author": "",
  "license": "MIT"
}
